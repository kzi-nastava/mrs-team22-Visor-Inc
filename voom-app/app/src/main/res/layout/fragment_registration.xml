<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/fullscreenBackgroundColor"
    android:theme="@style/ThemeOverlay.VoomApp.FullscreenContainer"
    android:id="@+id/fragment_registration"
    tools:context=".unauthenticated.registration.RegistrationFragment">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/registration_nav_host"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="false"
        app:navGraph="@navigation/registration_nav_graph"
        android:name="androidx.navigation.fragment.NavHostFragment"/>

</FrameLayout>